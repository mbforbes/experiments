hxswf:
	haxe \
		-main BasicSimulation -swf BasicSimulation.swf -swf-header 800:600:60:333333 -swf-version 11 \
		-lib nape -D haxe3 -D NAPE_RELEASE_BUILD -dce full

hxhtml5:
	haxe \
		-main BasicSimulation -js BasicSimulation.js -swf-header 800:600:60:333333 -swf-version 11 \
		-lib nape -D haxe3 -D NAPE_RELEASE_BUILD -dce full	
